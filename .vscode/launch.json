    "version": "0.2.0",
    "configurations": [
        {
            "name": "Execute main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--debug=DEBUG",
                "-e=2",
                "-b=4",
                "-ee=10",
                "-m=base",
                "-d=race",
                "-se",
                "LoRA",
                "-r=1",
                "-a=1",
                //"${command:pickArgs}"
            ],
            "justMyCode": false
        }
    ]
}